      leaf out-pause-pkts {
        type yang:zero-based-counter64;
        description
          "
          The total number of pause packets transmitted out of the interface.
          ";
      }
      leaf in-pkts {
        type yang:zero-based-counter64;
        description
          "
          The number of packets received on the interface.
          ";
      }
      leaf in-octets {
        type yang:zero-based-counter64;
        description
          "
          The total number of octets received on the interface.
          ";
      }
      leaf in-unicast-pkts {
        type yang:zero-based-counter64;
        description
          "
          The number of unicast packets received on the interface.
          ";
      }
      leaf in-multicast-pkts {
        type yang:zero-based-counter64;
        description
          "
          The number of multicast packets received on the interface.
          ";
      }
      leaf in-broadcast-pkts {
        type yang:zero-based-counter64;
        description
          "
          The number of broadcast packets received on the interface.
          ";
      }
      leaf in-vlan-pkts {
        type yang:zero-based-counter64;
        description
          "
          The total number of vlan packets received on the interface.
          ";
      }
      leaf in-pause-pkts {
        type yang:zero-based-counter64;
        description
          "
          The total number of pause packets received on the interface.
          ";
      }
      leaf carrier-loss-error {
        type yang:zero-based-counter64;
        description
          "
          The total number of carrier loss errors.
          ";
      }
      leaf crc-error {
        type yang:zero-based-counter64;
        description
          "
          The total number of Cyclic Redundancy Check (CRC) errors.
          ";
      }
      leaf jabber-error {
        type yang:zero-based-counter64;
        description
          "
          The total number of jabber errors.
          ";
      }
    }
    list fibrechannel-lag {
      if-feature fibrechannel:fibrechannel_lag_platform;
      key "name";
      description
        "
        The list of fibrechannel LAG interfaces on the switch, their
        related configuration and operational state.
        ";
      leaf name {
        type string {
          length "1..31";
          pattern "[0-9a-zA-Z_-]+";
        }
        description
         "
         The name of the fibrechannel LAG interface.
         ";
      }
      leaf number {
        type int32 {
          range "-1..96";
        }
        config false;
        description
         "
         The port-number of the fibrechannel LAG interface.
         ";
      }
      leaf port-index {
        type fibrechannel:user-port-number-type;
        config false;
        description
          "
          The virtual port index created and assigned to the
          fibrechannel LAG interface.
          ";
      }
      leaf port-wwn {
        type fibrechannel:wwn-type;
        config false;
        description
          "
          The world wide name (WWN) of the fibrechannel LAG interface.
          ";
      }
      leaf admin-state-enabled {
        type boolean;
        description
          "
          Indicates the admin state of the fibrechannel LAG interface.
          The possible values are:

           false : fibrechannel LAG is disabled.
           true  : fibrechannel LAG is enabled.

          ";
      }
      leaf link-status {
        type fc-lag-link-status-type;
        config false;
        description
          "
          Operational link status of the fibrechannel LAG interface.
          ";
      }
      container member-ports {
        description
          "The fibrechannel LAG members.";
        leaf-list port {
          type fibrechannel:slot-port-name-type;
          description
            "
            The list of fibrechannel ports that are members of
            the fibrechannel LAG interface.
            ";
        }
      }
    }
  }
